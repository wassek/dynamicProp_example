<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <dynamicpropexample j:dependencies="default"
                          j:modulePriority="0"
                          j:moduleType="module"
                          j:title="dynamicProp_example"
                          jcr:mixinTypes="jmix:hasExternalProviderExtension"
                          jcr:primaryType="jnt:module">
         <sources jcr:mixinTypes="jmix:hasExternalProviderExtension"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-dynamicpropexample-1.0-SNAPSHOT">
            <pom.xml jcr:primaryType="jnt:xmlFile"/>
            <src jcr:mixinTypes="jmix:hasExternalProviderExtension"
                 jcr:primaryType="jnt:folder">
               <main jcr:mixinTypes="jmix:hasExternalProviderExtension"
                     jcr:primaryType="jnt:folder">
                  <java jcr:mixinTypes="jmix:hasExternalProviderExtension"
                        jcr:primaryType="jnt:javaPackageFolder">
                     <org jcr:mixinTypes="jmix:hasExternalProviderExtension"
                          jcr:primaryType="jnt:javaPackageFolder">
                        <jahia jcr:mixinTypes="jmix:hasExternalProviderExtension"
                               jcr:primaryType="jnt:folder">
                           <modules jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                    jcr:primaryType="jnt:javaPackageFolder">
                              <dynamicprop jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                           jcr:primaryType="jnt:javaPackageFolder">
                                 <flow jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                       jcr:primaryType="jnt:javaPackageFolder">
                                    <DynamicPropFlowHandler.java jcr:primaryType="jnt:javaSourceFile"/>
                                 </flow>
                                 <init jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                       jcr:primaryType="jnt:javaPackageFolder">
                                    <DynamicPropToolbarInitializer.java jcr:primaryType="jnt:javaSourceFile"/>
                                 </init>
                              </dynamicprop>
                           </modules>
                        </jahia>
                     </org>
                  </java>
                  <resources jcr:mixinTypes="jmix:hasExternalProviderExtension"
                             jcr:primaryType="jnt:folder">
                     <javascript jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                 jcr:primaryType="jnt:javascriptFolder">
                        <dynamicprop.js jcr:primaryType="jnt:javascriptFile"/>
                     </javascript>
                     <jnt_dynamicPropSettings jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                              jcr:primaryType="jnt:nodeTypeFolder">
                        <html jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:templateTypeFolder">
                           <dynamicPropSettings.flow jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                                     jcr:primaryType="jnt:folder">
                              <flow.xml jcr:primaryType="jnt:xmlFile"/>
                              <view.jsp jcr:primaryType="jnt:viewFile"/>
                           </dynamicPropSettings.flow>
                        </html>
                     </jnt_dynamicPropSettings>
                     <jnt_dynamic_prop jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                       jcr:primaryType="jnt:nodeTypeFolder">
                        <html jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:templateTypeFolder">
                           <dynamic_prop.jsp jcr:primaryType="jnt:viewFile"/>
                        </html>
                     </jnt_dynamic_prop>
                     <META-INF jcr:mixinTypes="jmix:hasExternalProviderExtension"
                               jcr:primaryType="jnt:metaInfFolder">
                        <spring jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:folder">
                           <dynamicpropexample.xml jcr:primaryType="jnt:xmlFile"/>
                        </spring>
                     </META-INF>
                  </resources>
               </main>
            </src>
         </sources>
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <site-settings-base j:view="siteSettings" jcr:primaryType="jnt:template">
               <dynamicProp j:applyOn="jnt:virtualsite" jcr:primaryType="jnt:contentTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Dynamic Properties"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <set-dynamic-properties jcr:mixinTypes="jmix:renderable"
                                             jcr:primaryType="jnt:dynamicPropSettings">
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_de jcr:language="de"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_pt jcr:language="pt"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_it jcr:language="it"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_fr jcr:language="fr"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_es jcr:language="es"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Set Dynamic Properties"/>
                        <j:translation_de_DE jcr:language="de_DE"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Set Dynamic Properties"/>
                     </set-dynamic-properties>
                  </pagecontent>
               </dynamicProp>
            </site-settings-base>
         </templates>
      </dynamicpropexample>
   </modules>
</content>